{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "adminPortal": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/admin-portal",
      "sourceRoot": "projects/admin-portal/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/admin-portal",
            "index": "projects/admin-portal/src/index.html",
            "browser": "projects/admin-portal/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/admin-portal/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/admin-portal/src/favicon.ico",
              "projects/admin-portal/src/assets"
            ],
            "styles": [
              "projects/admin-portal/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "adminPortal:build:production"
            },
            "development": {
              "buildTarget": "adminPortal:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "adminPortal:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/admin-portal/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/admin-portal/src/favicon.ico",
              "projects/admin-portal/src/assets"
            ],
            "styles": [
              "projects/admin-portal/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "customerPortal": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/customer-portal",
      "sourceRoot": "projects/customer-portal/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/customer-portal",
            "index": "projects/customer-portal/src/index.html",
            "browser": "projects/customer-portal/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/customer-portal/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/customer-portal/src/favicon.ico",
              "projects/customer-portal/src/assets"
            ],
            "styles": [
              "projects/customer-portal/src/styles.scss",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "@angular/material/prebuilt-themes/indigo-pink.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "customerPortal:build:production"
            },
            "development": {
              "buildTarget": "customerPortal:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "customerPortal:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/customer-portal/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/customer-portal/src/favicon.ico",
              "projects/customer-portal/src/assets"
            ],
            "styles": [
              "projects/customer-portal/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "shardLib": {
      "projectType": "library",
      "root": "projects/shard-lib",
      "sourceRoot": "projects/shard-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/shard-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/shard-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/shard-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/shard-lib/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
